<UserControl x:Class="SyntaxEditor.SyntaxBrowserGraph"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:m="clr-namespace:SyntaxEditor.Model"
             xmlns:vm="clr-namespace:SyntaxEditor.ViewModel"
             xmlns:sample="clr-namespace:SyntaxEditor.ViewModel.Sample"
             xmlns:c="clr-namespace:SyntaxEditor.Controls"
             mc:Ignorable="d"
             d:DesignHeight="450"
             d:DesignWidth="340">
    <d:UserControl.DataContext>
        <sample:SyntaxBrowser />
    </d:UserControl.DataContext>
    <c:GraphLayout DataContext="{Binding Graph, Mode=OneWay}" />
</UserControl>
